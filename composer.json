{
    "name": "survos/doc-bundle",
    "description": "Build Sphinx documentation for a Symfony project, using Twig templates",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Tac Tacelosky",
            "email": "tacman@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/config": "^6.1.0",
        "symfony/console": "^6.1.0",
        "symfony/dependency-injection": "^6.1.0",
        "symfony/http-kernel": "^6.1.0",
        "symfony/options-resolver": "^6.1.0",
        "twig/twig": "^3.4",
        "symfony/finder": "^6.1.0"
    },
    "autoload": {
        "psr-4": {
            "Survos\\DocBundle\\": "src/"
        }
    },
    "extra": {
        "symfony": {
            "require": "^6.1.0"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.8",
        "rector/rector": "^0.13.9",
        "codeception/codeception": "^5.0",
        "codeception/module-asserts": "^3.0",
        "codeception/module-doctrine2": "^3.0",
        "codeception/module-phpbrowser": "^3.0",
        "codeception/module-symfony": "^3.0 | *@dev"
    },
    "conflict": {
        "survos/maker-bundle": "<1.2.49",
        "survos/html-prettify-bundle": "<1.2.49",
        "survos/barcode-bundle": "<1.2.49",
        "survos/api-grid-bundle": "<1.2.49",
        "survos/bootstrap-bundle": "<1.2.49",
        "survos/inspection-bundle": "<1.2.49",
        "survos/faker-bundle": "<1.2.49",
        "survos/grid-bundle": "<1.2.49",
        "survos/core-bundle": "<1.2.49",
        "survos/tree-bundle": "<1.2.49",
        "survos/auth-bundle": "<1.2.49",
        "survos/workflow-helper-bundle": "<1.2.49",
        "survos/crawler-bundle": "<1.2.49",
        "survos/location-bundle": "<1.2.49",
        "survos/command-bundle": "<1.2.49",
        "survos/ruler-bundle": "<1.2.49",
        "survos/providence-bundle": "<1.2.49",
        "survos/stripe-product": "<1.2.49",
        "survos/wiki-bundle": "<1.2.49",
        "survos/import-bridge": "<1.2.49",
        "survos/grid-group-bundle": "<1.2.49"
    }
}
